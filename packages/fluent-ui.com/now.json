{
  "version": 2,
  "name": "fluent-ui.com",
  "env": {
    "GATSBY_ALGOLIA_APP_ID": "@GATSBY_ALGOLIA_APP_ID",
    "GATSBY_ALGOLIA_SEARCH_KEY": "@GATSBY_ALGOLIA_APP_ID",
    "ALGOLIA_ADMIN_KEY": "@GATSBY_ALGOLIA_APP_ID"
  },
  "build": {
    "env": {
      "GATSBY_ALGOLIA_APP_ID": "@GATSBY_ALGOLIA_APP_ID",
      "GATSBY_ALGOLIA_SEARCH_KEY": "@GATSBY_ALGOLIA_APP_ID",
      "ALGOLIA_ADMIN_KEY": "@GATSBY_ALGOLIA_APP_ID"
    }
  },
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": { "distDir": "public" }
    }
  ]
}
