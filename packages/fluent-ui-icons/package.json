{
  "name": "@fluent-ui/icons",
  "private": false,
  "version": "0.0.1",
  "description": "Fluent Design System Svg Icon.",
  "author": "chenyueban <jasonchan0527@gmail.com>",
  "keywords": [
    "react",
    "fluent-design",
    "fluent-ui",
    "icons"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/chenyueban/fluent-ui",
    "directory": "packages/fluent-ui"
  },
  "homepage": "https://fluent-ui.com/",
  "bugs": {
    "url": "https://github.com/chenyueban/fluent-ui/issues"
  },
  "license": "MIT",
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "prebuild": "rimraf build",
    "compile": "ts-node ./builder",
    "build": "yarn compile && yarn build:cjs && yarn build:es && yarn build:umd && yarn build:copy-files",
    "build:cjs": "cross-env NODE_ENV=production tsc --module CommonJS --outDir ./build",
    "build:es": "cross-env NODE_ENV=production tsc --module ES6 --outDir ./build/es",
    "build:umd": "cross-env NODE_ENV=production tsc --module UMD --outDir ./build/umd",
    "build:copy-files": "ts-node ../../scripts/copy-files.ts",
    "start": "cross-env NODE_ENV=production tsc -w --module ES6 --outDir ./build/es"
  },
  "peerDependencies": {
    "react": ">= 16.8.0",
    "react-dom": ">= 16.8.0",
    "styled-components": ">= 4.X"
  },
  "devDependencies": {
    "@types/svgo": "^1.2.0",
    "htm": "^2.1.1",
    "omit-deep-lodash": "^1.1.4",
    "svgo": "^1.2.2"
  },
  "main": "build/index.js",
  "module": "build/es/index.js",
  "typings": "build/index.d.ts"
}
