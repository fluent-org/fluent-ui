{
  "version": 2,
  "alias": "fluent-windows.com",
  "env": {
    "GATSBY_ALGOLIA_APP_ID": "@gatsby_algolia_app_id",
    "GATSBY_ALGOLIA_SEARCH_KEY": "@gatsby_algolia_search_key",
    "ALGOLIA_ADMIN_KEY": "@algolia_admin_key"
  },
  "build": {
    "env": {
      "GATSBY_ALGOLIA_APP_ID": "@gatsby_algolia_app_id",
      "GATSBY_ALGOLIA_SEARCH_KEY": "@gatsby_algolia_search_key",
      "ALGOLIA_ADMIN_KEY": "@algolia_admin_key"
    }
  },
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": { "distDir": "public" }
    }
  ]
}
